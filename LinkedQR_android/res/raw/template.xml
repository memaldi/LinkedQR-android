<?xml version="1.0" encoding="UTF-8"?>
<template>
	<prefixes>
	    <prefixItem>
	        <prefix>foaf</prefix>
	        <uri>http://xmlns.com/foaf/0.1/</uri>
	    </prefixItem>
	    <prefixItem>
	        <prefix>rel</prefix>
	        <uri>http://purl.org/vocab/relationship/</uri>
	    </prefixItem>
	</prefixes>
	<pageItem type="foaf:Person" class="eu.deustotech.internet.linkedqr.android.layout.Person">
	    <items>
            <item id="depiction">
                <property>foaf:depiction</property>
            </item>
		    <item id="name" main="true">
		        <property>foaf:name</property>
		    </item>
		    <item id="email">
		        <property>foaf:mbox</property>
		    </item>
		    <item id="workmates" linkable="true">
		        <property>rel:worksWith</property>
		    </item>
	    </items>
	</pageItem>
</template>